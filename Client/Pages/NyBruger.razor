@page "/nybruger"

<h3>Tilføj Ny Bruger</h3>

<div class="row" style="margin-bottom: 30px">
    <div class="col-md-6 mb-3">
        <label for="fullName">Fulde navn</label>
        <input type="text" class="form-control" id="fullName">
    </div>
    <div class="col-md-6 mb-3">
        <label for="email">Email</label>
        <input type="email" class="form-control" id="email">
    </div>
    <div class="col-md-6 mb-3">
        <label for="phoneNumber">Telefonnummer</label>
        <input type="text" class="form-control" id="phoneNumber">
    </div>
    <div class="col-md-6 mb-3">
        <label for="memberNumber">Krævnummer</label>
        <input type="text" class="form-control" id="memberNumber">
    </div>
    <div class="col-md-6 mb-3">
        <label for="password">Adgangskode</label>
        <input type="password" class="form-control" id="password">
    </div>
</div>

<div class="row" style="margin-bottom: 50px; text-align: center">
    <div>
        <button type="submit" class="btn btn-success" style="margin-top: 10px">Tilføj</button>
    </div>
</div>

